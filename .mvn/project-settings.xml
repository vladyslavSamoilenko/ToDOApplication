<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">


    <profiles>
        <profile>
            <id>autograding-profile</id>
            <repositories>
                <repository>
                    <id>autograding-test-maven-repo</id>
                    <name>autograding-test-maven-repo</name>
                    <url>https://github.com/EPAM-Online-Courses/efs-task9-todo-app/releases/download/1/</url>
                    <snapshots>
                        <enabled>true</enabled>
                        <updatePolicy>daily</updatePolicy>
                    </snapshots>
                </repository>
            </repositories>
        </profile>
    </profiles>

    <activeProfiles>
        <activeProfile>autograding-profile</activeProfile>
    </activeProfiles>
</settings>
